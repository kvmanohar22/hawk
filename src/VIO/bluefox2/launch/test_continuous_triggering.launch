<launch>
    <arg name="device" default="26807563"/>
    <arg name="expose_us" default="16000"/>
    <arg name="view" default="false"/>

    <!-- Start camera driver -->
    <include file="$(find bluefox2)/launch/single_node.launch">
        <arg name="record_cameras" value="false"/>
        <arg name="expose_us" value="$(arg expose_us)"/>
        <arg name="device" value="$(arg device)"/>
        <arg name="view" value="$(arg view)"/>
        <arg name="rate" value="150"/>
        <arg name="request" value="100"/>
        <arg name="gain_db" value="12.0"/>
        <arg name="hdr" value="false"/>
        <arg name="mm" value="3"/>
        <arg name="aec" value="false"/>
        <arg name="agc" value="false"/>
        <arg name="ctm" value="0"/>
        <arg name="cts" value="-1"/>
    </include>

</launch>
