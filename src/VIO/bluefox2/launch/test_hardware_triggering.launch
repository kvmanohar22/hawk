<launch>
    <arg name="expose_us" default="890"/>

    <!-- Start mavros -->
    <include file="$(find mavros)/launch/px4.launch">
        <arg name="fcu_url" value="/dev/ttyOffboard:921600"/>
    </include>

    <!-- Start camera driver -->
    <include file="$(find bluefox2)/launch/single_node.launch">
        <arg name="record_cameras" value="true"/>
        <arg name="expose_us" value="$(arg expose_us)"/>
        <arg name="rate" value="150"/>
        <arg name="request" value="100"/>
        <arg name="gain_db" value="1.0"/>
        <arg name="hdr" value="true"/>
        <arg name="mm" value="3"/>
        <arg name="aec" value="false"/>
        <arg name="agc" value="false"/>
    </include>

</launch>
