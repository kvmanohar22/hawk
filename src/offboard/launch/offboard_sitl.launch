<launch>
  <!-- Start mavros -->
  <include file="$(find mavros)/launch/px4.launch">
    <arg name="fcu_url" value="/dev/ttyOffboard:921600"/>
  </include>

  <!-- start offboard node to communicate with mavros -->
  <node pkg="offboard" type="test_offboard_basic" name="offboard" output="screen"></node> 
</launch>
